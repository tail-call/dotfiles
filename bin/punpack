#!/bin/bash

file="$1"

if [[ -z "$file" ]]; then >&2 cat << EOF
Usage: punpack Photos.zip
EOF
exit 1
fi

tempdir=$(mktemp -d)

#unzip -e "$file" -d "$tempdir"

#(for file in $tempdir/*.png
(for file in *.png
 do
     >&2 echo Converting $file.
     png2pnm $file | tail -n+4
 done) | tar -xzf -
# done) | tar -xf -
# done) > out.gpg

>&2 echo Cleaning up...
rm -rf $tempdir

>&2 echo Done.
